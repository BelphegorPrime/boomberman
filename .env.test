PORT=3000
WEBHOOK_URL=https://your.webhook.endpoint/alert

ENABLE_AI_FAKE_RESPONSES=false
AI_PROVIDER=ollama
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2

BAN_FILE_PATH=./test/data/banned.json
LOG_FILE_PATH=./test/data/app.log
AI_FAKE_RESPONSES_PATH=./test/data/fakeResponses.jsonl

MAX_FAKE_RESPONSE_FILESIZE_BYTES=50000

AI_PRE_POPULATE_CACHE=false

# GeoIP Database Paths (optional - defaults to ./src/data/geoip/ if not specified)
# GEOLITE2_CITY_DB_PATH=./src/data/geoip/GeoLite2-City.mmdb
# GEOLITE2_ASN_DB_PATH=./src/data/geoip/GeoLite2-ASN.mmdb

# Enhanced Bot Detection Configuration
# Main Settings
BOT_DETECTION_ENABLED=true

# Scoring Weights (must sum to 1.0)
BOT_DETECTION_WEIGHT_FINGERPRINT=0.3
BOT_DETECTION_WEIGHT_BEHAVIORAL=0.3
BOT_DETECTION_WEIGHT_GEOGRAPHIC=0.2
BOT_DETECTION_WEIGHT_REPUTATION=0.2

# Detection Thresholds (0-100)
BOT_DETECTION_THRESHOLD_SUSPICIOUS=30
BOT_DETECTION_THRESHOLD_HIGH_RISK=70

# Behavioral Analysis Settings
BOT_DETECTION_MIN_HUMAN_INTERVAL=500
BOT_DETECTION_MAX_CONSISTENCY=0.8
BOT_DETECTION_SESSION_TIMEOUT=1800000

# Geographic Analysis Settings
BOT_DETECTION_HIGH_RISK_COUNTRIES=CN,RU,KP,IR
BOT_DETECTION_VPN_PENALTY=20
BOT_DETECTION_HOSTING_PENALTY=15

# Whitelist Settings
BOT_DETECTION_WHITELIST_IPS=127.0.0.1,::1
BOT_DETECTION_WHITELIST_ASNS=
BOT_DETECTION_REQUIRED_HEADERS=Accept,Accept-Language,Accept-Encoding,Connection,Cache-Control,User-Agent